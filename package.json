{
  "name": "ai-account-planner",
  "version": "1.0.0",
  "description": "AI-powered account planning system with external data integration",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node src/index.js",
    "start:prod": "NODE_ENV=production node --max-old-space-size=4096 src/index.js",
    "dev": "NODE_ENV=development node --watch src/workflowServer.js",
    "workflow": "NODE_ENV=production node src/workflowServer.js",
    "workflow:dev": "NODE_ENV=development node --watch src/workflowServer.js",
    "demo": "NODE_ENV=production node demo.js",
    "demo:stripe": "NODE_ENV=production node demo.js Stripe",
    "demo:acme": "NODE_ENV=production node demo.js \"Acme Retail\"",
    "test": "NODE_ENV=test node --test tests/*.test.js",
    "test:coverage": "NODE_ENV=test c8 node --test tests/*.test.js",
    "lint": "eslint src/",
    "typecheck": "node --check src/**/*.js",
    "validate": "npm run typecheck && npm run lint && npm test",
    "deploy": "npm run validate && npm run start:prod",
    "graphrag:start": "./scripts/start-graphrag.sh",
    "graphrag:stop": "docker-compose down",
    "graphrag:logs": "docker-compose logs -f",
    "graphrag:build": "docker-compose build graphrag-service",
    "neo4j:start": "docker-compose up -d neo4j",
    "neo4j:stop": "docker-compose stop neo4j",
    "neo4j:browser": "open http://localhost:7474"
  },
  "keywords": [
    "ai",
    "account-planning",
    "mcp",
    "data-integration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@bull-board/api": "^5.23.0",
    "@bull-board/express": "^5.23.0",
    "@slack/web-api": "^7.10.0",
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.11.0",
    "bullmq": "^5.58.5",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "ioredis": "^5.4.1",
    "klavis": "^1.0.1",
    "multer": "^2.0.2",
    "n8n": "^1.110.1",
    "neo4j-driver": "^5.28.1",
    "nodemailer": "^7.0.6",
    "openai": "^4.104.0",
    "pg": "^8.16.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
